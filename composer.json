{
    "name": "littlesqx/aint-queue",
    "description": "A async-queue library built on top swoole, flexable multi-consumer, coroutine supported.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "littlesqx",
            "email": "littlesqx@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-swoole": ">=4.3",
        "predis/predis": "^1.1",
        "psr/log": "^1.1",
        "monolog/monolog": "^1.24",
        "jeremeamia/superclosure": "^2.4",
        "symfony/console": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Littlesqx\\AintQueue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "suggest": {
        "php": ">=7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "swoole/ide-helper": "@dev"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "bin": [
        "bin/aint-queue"
    ]
}
